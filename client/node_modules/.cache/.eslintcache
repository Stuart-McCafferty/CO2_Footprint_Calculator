.js",["48"],[],"import React, {useState} from 'react';\nimport styled from 'styled-components';\n\nconst QuizFood = () => {\n\nconst [VegIsShown, setVegIsShown] = useState(false);\nconst [PescIsShown, setPescIsShown] = useState(false);\nconst [MeatIsShown, setMeatIsShown] = useState(false);\nconst [number, setNumber] = useState(0)\n\nconst handleInc = (event) => {\n  event.preventDefault();\n  let newNum = number + 1; \n  setNumber(newNum);\n}\n\nconst handleDec = (event) => {\n  event.preventDefault();\n  let newNum = number - 1; \n  setNumber(newNum);\n}\n\nfunction MeatOption () {\n  return (\n    <div>\n    <h2>What products do you eat in an average week?</h2>\n    <form>\n        <h3>Cheese</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Eggs</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Milk</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Tinned fish</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Fresh fish</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Shellfish</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Beef</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Pork</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Chicken</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n\n        <h3>Lamb</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n\n        <h3>Turkey</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n\n    </form>\n</div>\n  )\n}\n\nfunction PescOption(){\n  return (\n<div>\n    <h2>What products do you eat in an average week?</h2>\n    <form>\n        <h3>Cheese</h3>\n        <div>\n              <p>How many portions a week?</p>\n              <p>{number}</p>\n              <button onClick={handleInc}>+</button>\n              <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Eggs</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Milk</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Tinned fish</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Fresh fish</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n        <h3>Shellfish</h3>\n        <div>\n            <p>How many portions a week?</p>\n            <p>{number}</p>\n            <button onClick={handleInc}>+</button>\n            <button onClick={handleDec}>-</button>\n        </div>\n\n    </form>\n    </div>\n  )\n}\n\nfunction VegOption () {\n  return (\n      <div>\n          <h2>What products do you eat in an average week?</h2>\n          <form>\n              <h3>Cheese</h3>\n              <div>\n                  <p>How many portions a week?</p>\n                  <p>{number}</p>\n                  <button onClick={handleInc}>+</button>\n                  <button onClick={handleDec}>-</button>\n              </div>\n\n              <h3>Eggs</h3>\n              <div>\n                  <p>How many portions a week?</p>\n                  <p>{number}</p>\n                  <button onClick={handleInc}>+</button>\n                  <button onClick={handleDec}>-</button>\n              </div>\n\n              <h3>Milk</h3>\n              <div>\n                  <p>How many portions a week?</p>\n                  <p>{number}</p>\n                  <button onClick={handleInc}>+</button>\n                  <button onClick={handleDec}>-</button>\n              </div>\n          </form>\n      </div>\n  )\n}\n\n  return (\n      <form>\n        <h2>How would you describe your eating habits?</h2>\n            <button>Vegan</button>\n            <button onClick={(event) => {\n              event.preventDefault();\n              setVegIsShown(!VegIsShown)}}>\n              {`${VegIsShown ? 'Vegetarian' : 'Vegetarian'}`}\n            </button>\n            <button onClick={(event) => {\n              event.preventDefault();\n              setPescIsShown(!PescIsShown)}}>\n              {`${PescIsShown ? 'Pescatarian' : 'Pescatarian'}`}</button>\n            <button onClick={(event) => {\n              event.preventDefault();\n              setMeatIsShown(!MeatIsShown)}}>\n              {`${MeatIsShown ? 'Meat Eater' : 'Meat Eater'}`}</button>\n            {VegIsShown && <VegOption/>}\n            {PescIsShown && <PescOption/>}\n            {MeatIsShown && <MeatOption/>}\n      </form>\n  )\n\n}\n\n\nexport default QuizFood;","/Users/lawrie/Documents/codeclan_E59/project_week/CO2_Footprint_Calculator/client/src/components/QuizVeggie.js",[],[],{"ruleId":"49","severity":1,"message":"50","line":2,"column":8,"nodeType":"51","messageId":"52","endLine":2,"endColumn":14},"no-unused-vars","'styled' is defined but never used.","Identifier","unusedVar"]